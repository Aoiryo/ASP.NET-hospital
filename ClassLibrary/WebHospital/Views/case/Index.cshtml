@model IEnumerable<ClassLibrary.medicalHistory>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.department)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.medicalPersonnelID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.diagnosisResult)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.department)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.medicalPersonnelID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.diagnosisResult)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.medicalPersonnelID, time = item.diagnosisTime}) |
            @Html.ActionLink("Details", "Details", new { id = item.medicalPersonnelID, time = item.diagnosisTime }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.medicalPersonnelID, time = item.diagnosisTime })
        </td>
    </tr>
}

</table>
